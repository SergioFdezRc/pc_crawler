<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pc.crawler.controller.FatCtrl">
    <children>
        <ListView fx:id="lv_fat" prefHeight="200.0" prefWidth="600.0" />
        <Button fx:id="btn_exp_fat" layoutX="524.0" layoutY="208.0" mnemonicParsing="false" styleClass="info" text="Exportar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="btn_cancel_fat" cancelButton="true" layoutX="438.0" layoutY="208.0" mnemonicParsing="false" styleClass="danger" text="Cancelar">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
</AnchorPane>
